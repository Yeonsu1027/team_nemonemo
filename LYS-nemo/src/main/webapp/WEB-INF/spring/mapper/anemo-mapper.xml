<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.callor.hello.dao.ANemoDao">

	<select id="selectAll">
		SELECT * FROM tbl_nemo
	</select>
	
	<select id="findByRow">
		SELECT *
		FROM tbl_nemo
		WHERE n_num = #{n_num} AND n_row_num = #{n_row_num}
	</select>
	
	<insert id="insert">
	</insert>
	<update id="update">
	</update>
	<delete id="delete"></delete>

</mapper> 